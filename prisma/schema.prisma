generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tipo {
  id   Int    @id @default(autoincrement())
  nome String @unique
}


model Pokemon {
  id           Int     @id @default(autoincrement())
  nome         String  @unique
  peso         Float   @default(0.0)
  altura       Float   @default(0.0)
  experiencia  Int     @default(0)
  inicial      Boolean @default(false)
}



